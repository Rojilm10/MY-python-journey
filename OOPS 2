class Students:
    def __init__(self, name, roll):
        self.name = name
        self.roll = roll

    def my_name(self):
        return self.name
    def my_roll(self):
        return self.roll
   # def Char(self):
       # print("food")
d = Students("roji", 55)
print(d.my_name())
print(d.my_roll())class Students:
